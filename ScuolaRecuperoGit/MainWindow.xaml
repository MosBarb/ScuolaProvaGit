<Window x:Class="ScuolaRecuperoGit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScuolaRecuperoGit"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="440*"/>
            <ColumnDefinition Width="259*"/>
            <ColumnDefinition Width="101*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="txboxBiennioTriennio" HorizontalAlignment="Left" Margin="280,64,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Height="18" TextChanged="TextBox_TextChanged"/>
        <Button Content="controllo Biennio/Triennio" HorizontalAlignment="Left" Margin="10,58,0,0" VerticalAlignment="Top" Height="24" Width="254" RenderTransformOrigin="0.479,0.582"/>
        <ListView x:Name="listStudenti" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="39,83,39,62" Grid.Column="1">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
